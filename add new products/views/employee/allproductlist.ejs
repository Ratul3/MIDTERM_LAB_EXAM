<!DOCTYPE html>
<html>
<head>
	<title>All Product List</title>
</head>
<body>
		<form method="post">
			<fieldset>
				<legend>All Product List</legend>
			<table id="table" border="1">
		<tr>
			<th>id</th>
			<th>product name</th>
				
			<th>operations</th>
		
		</tr>
      <% for(var i=0; i < results.length; i++){ %>
		
		<tr>
			<td><%= results[i].id %></td>
			<td><%= results[i].name %></td>
			
			
			<td><a href="/employee/update/<%= results[i].id %>">Edit</a> | <a href="/employee/delete/<%= results[i].id %>"> Delete </a></td>
		</tr>
		<% } %>
	</table>

	
	<br/>
	
	<a href="/home">Back</a>
			</fieldset>
		</form>
</body>
</html>